   "source": [
    "from lxml import etree\n",
    "\n",
    "table = etree.HTML(open(\"data.html\", \"r\").read()).find(\"body/table\")\n",
    "rows = iter(table)\n",
    "headers = [col.text for col in next(rows)]\n",
    "for row in rows:\n",
    "    values = [col.text for col in row]\n",
    "    print(dict(zip(headers, values)))"
